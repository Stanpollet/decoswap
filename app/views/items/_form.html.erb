<%= simple_form_for(@item) do |f| %>
  <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :category_item %>
      <%= f.input :size %>
      <%= f.input :image_url %>
      <%= f.input :description %>

  </div>
    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>


