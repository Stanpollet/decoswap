    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
          <ul class="nav nav-sidebar">
            <li> <%= link_to 'Dashboard', dashboard_path %>  </li>
            <li class="active"> <%= link_to 'Manage Items', manager_items_dashboard_path %>  </li>
            <li> <%= link_to 'Stats', stats_dashboard_path %>  </li>
            <li> <%= link_to 'Account Settings', account_settings_dashboard_path %>  </li>
            <li> <br></li>
            <li> <%= link_to 'Add New item', new_item_path %> </li>
          </ul>
        </div>
        <%= link_to 'Add New item', new_item_path, class: "btn btn-lg btn-primary", id: "new_item_btn" %>
        <div id="relative_div">
          <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <h1 class="page-header">Manage my products</h1>
            <div class="table-responsive">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th>#id</th>
                    <th>Item</th>
                    <th>Customer</th>
                    <th>Status</th>
                    <th>Price</th>
                    <th>Product Page</th>
                  </tr>
                </thead>
                <tbody>
                  <% @user.items.each do |item| %>
                  <tr>
                    <td><%= item.id %></td>
                    <td><%= item.name %></td>
                    <td><%= item.size %></td>
                    <td><%= item.status %></td>
                    <td><%= item.price %></td>
                    <td> <%= link_to 'see', item_path(item) %></td>
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
