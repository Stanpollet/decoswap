<div class="container">
	<div class="row">
		<div class="col-xs-12 text-center">
			<h1>Welcome to the Swap Side of life</h1>
		</div>
	</div>
	<div class="swap-flex">
		<div class="swap-item">
			<%= render "items/card", item: @item_return %>
		</div>
		<div class="swap-item text-center">
			<i class="fa fa-long-arrow-right" aria-hidden="true"></i>
			<p>Swapping...</p>
			<i class="fa fa-long-arrow-left" aria-hidden="true"></i>
		</div>
		<div class="swap-item"><%= render "items/card", item: @item_booked %></div>
	</div>
</div>